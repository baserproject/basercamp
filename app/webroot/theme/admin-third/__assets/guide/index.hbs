  <script type="text/javascript">
    $(function () {
      $("#BlogPostPublishBeginTime").timepicker({'timeFormat': 'H:i'});
      $("#BlogPostPublishBeginDate").change(BlogPostPublishBeginChangeResultHandler);
      $("#BlogPostPublishBeginTime").change(BlogPostPublishBeginChangeResultHandler);

      function BlogPostPublishBeginChangeResultHandler() {
        if ($("#BlogPostPublishBeginDate").val() && !$("#BlogPostPublishBeginTime").val()) {
          $("#BlogPostPublishBeginTime").val('00:00');
        }
        var value = $("#BlogPostPublishBeginDate").val().replace(/\//g, '-');
        if ($("#BlogPostPublishBeginTime").val()) {
          value += ' ' + $("#BlogPostPublishBeginTime").val();
        }
        $("#BlogPostPublishBegin").val(value);
      }
    });
  </script>
